module Transition (input Up, input Down, input Reset, input clock, input [3:0] CurrentState, output reg [3:0] NextState);
	always@(posedge clock)
	begin
		NextState = CurrentState;
		if(CurrentState == 4'd0)
		begin
			if (Up == 1 && Down == 0)
			begin
				NextState = 4'd1
			end
		
		
		end 
	end


endmodule
